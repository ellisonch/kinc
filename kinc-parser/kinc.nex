/[ \t\r\n]/ { /* Skip blanks and tabs. */ } 
/,/ { return ',' }
/\(/ { return '(' }
/\)/ { return ')' }
/\[/ { return '[' }
/\]/ { return ']' }
/:/ { return ':' }
/</ { return '<' }
/\.\.\.<\// { return TOK_CELL_RIGHT_OPEN }
/<\// { return TOK_CELL_RIGHT_CLOSED }
/>\.\.\./ { return TOK_CELL_LEFT_OPEN }
/>/ { return '>' }
/=>/ { return TOK_ARROW }
/~>/ { return TOK_KRA }
/configuration/ { return TOK_CONFIGURATION }
/rule/ { return TOK_RULE }
/[a-z][a-zA-Z]*/ {
	lval.str = string(yylex.Text())
	return TOK_LC_NAME
}
/[A-Z][a-zA-Z]*/ {
	lval.str = string(yylex.Text())
	return TOK_UC_NAME
}

